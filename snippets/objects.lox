class Bagel {
  init(taste, filling) {
    this.taste = taste;
    this.filling = filling;
  }

  describe() {
    return this.taste + " bagel with " + this.filling;
  }

  eat() {
    print "Crunch crunch crunch!";
  }
}

var bagel = Bagel("good", "cream cheese");
bagel.eat();

print bagel.describe();

bagel.taste = "amazing";
print bagel.describe();
