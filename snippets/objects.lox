class Bagel {
  init() {
    this.taste = "good";
    this.filling = "cream cheese";
  }

  describe() {
    return this.taste + " bagel with " + this.filling;
  }

  eat() {
    print "Crunch crunch crunch!";
  }
}

var bagel = Bagel();
bagel.eat();

print bagel.describe();

bagel.taste = "amazing";
print bagel.describe();
